<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/3.4/menu">
	<menu id="root-menu" label="Openbox">
		<separator label="Applications"/>
		<menu id="applications-menu" label="Internet">
			<item label="Chromium">
				<action name="Execute">
					<execute>
						chromium
					</execute>
				</action>
			</item>
			<item label="Thunderbird E-mail">
				<action name="Execute">
					<execute>
						thunderbird
					</execute>
				</action>
			</item>
			<item label="Pidgin Instant Messenger">
				<action name="Execute">
					<execute>
						pidgin
					</execute>
				</action>
			</item>
		</menu>
		<menu id="multimedia-menu" label="Multimedia">
			<item label="MPlayer">
				<action name="Execute">
					<execute>
						gnome-mplayer
					</execute>
				</action>
			</item>
			<item label="ncmpcpp">
				<action name="Execute">
					<execute>
						urxvt -e ncmpcpp
					</execute>
				</action>
			</item>
			<item label="Grooveshark">
				<action name="Execute">
					<execute>
						~/.bin/grooveshark.sh
					</execute>
				</action>
			</item>
		</menu>
		<menu id="programming-menu" label="Programming">
			<item label="Giggle">
				<action name="Execute">
					<execute>
						giggle
					</execute>
				</action>
			</item>
			<item label="Qt Creator">
				<action name="Execute">
					<execute>
						~/Projects/QtCreator/bin/qtcreator
					</execute>
				</action>
			</item>
			<item label="VIM Text Editor">
				<action name="Execute">
					<execute>
						urxvt -e vim
					</execute>
				</action>
			</item>
			<item label="GVIM Text Editor">
				<action name="Execute">
					<execute>
						gvim
					</execute>
				</action>
			</item>
			<item label="gEdit Text Editor">
				<action name="Execute">
					<execute>
						gedit
					</execute>
				</action>
			</item>
		</menu>
		<menu id="misc-menu" label="Miscallenous">
			<item label="Terminal">
				<action name="Execute">
					<execute>
						urxvt
					</execute>
				</action>
			</item>
			<item label="Wireshark">
				<action name="Execute">
					<execute>
						wireshark
					</execute>
				</action>
			</item>
			<item label="Terminal (tmux)">
				<action name="Execute">
					<execute>
						urxvt -e tmux
					</execute>
				</action>
			</item>
		</menu>
		<menu id="games-menu" label="Games">
			<item label="Steam">
				<action name="Execute">
					<execute>
						wine ~/.wine/drive_c/Program\ Files/Steam/Steam.exe
					</execute>
				</action>
			</item>
			<item label="Warsow">
				<action name="Execute">
					<execute>
						warsow
					</execute>
				</action>
			</item>
			<item label="Minecraft">
				<action name="Execute">
					<execute>
						minecraft
					</execute>
				</action>
			</item>
			<item label="Heroes of Newerth">
				<action name="Execute">
					<execute>
						~/.Heroes\ of\ Newerth/hon.sh
					</execute>
				</action>
			</item>
		</menu>
		<separator label="System"/>
		<menu id="system-menu" label="System">
			<item label="File Manager">
				<action name="Execute">
					<execute>
						thunar
					</execute>
				</action>
			</item>
		</menu>
		<menu id="openbox-menu" label="Openbox">
			<item label="Exit">
				<action name="Exit"/>
			</item>
			<item label="Restart">
				<action name="Restart"/>
			</item>
			<menu id="openbox-menu-configure" label="Configure">
				<item label="Menu">
					<action name="Execute">
						<execute>
							obmenu
						</execute>
					</action>
				</item>
				<item label="Windows">
					<action name="Execute">
						<execute>
							obconf
						</execute>
					</action>
				</item>
			</menu>
			<item label="Reconfigure">
				<action name="Reconfigure"/>
			</item>
		</menu>
	</menu>
</openbox_menu>
